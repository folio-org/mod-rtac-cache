{
  "type": "object",
  "description": "Real Time Availability Check (RTAC) holding summary details",
  "additionalProperties": false,
  "properties": {
    "instanceId": {
      "type": "string",
      "description": "The FOLIO instance id"
    },
    "status": {
      "type": "string",
      "description": "The status of the RTAC holdings summary"
    },
    "hasVolumes": {
      "type": "boolean",
      "description": "Indicates if the holding has volume information"
    },
    "copiesRemaining": {
      "description": "The number of copies remaining",
      "type": "object",
      "properties": {
        "total": {
          "type": "integer"
        },
        "available": {
          "type": "integer"
        }
      },
      "required": [
        "total",
        "available"
      ]
    }
  },
  "required": [
    "instanceId",
    "status",
    "hasVolumes",
    "copiesRemaining"
  ]
}
